{#
Need two arrays
    1) table_headers : Array of table headers
    2) table : 2D array of table data
#}
<div>
    <table class="table table-hover">
        {% block table_body %}
            <tr>
                {% for table_header in table_headers %}
                    <th>{{ table_header }}</th>
                {% endfor %}
            </tr>

            {% for row in table %}
                <tr>
                    {% for cell in row %}
                        <td>{{ cell }}</td>
                    {% endfor %}
                </tr>
            {% endfor %}
        {% endblock %}
    </table>
</div>